@model IEnumerable<ClinicaPokemon.Models.Prodotti>

@{
    ViewBag.Title = "Index";
}



<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
    
        <th>
            @Html.DisplayNameFor(model => model.DittaFornitrice.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsoProdotti.Uso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeProdotto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
   
        <td>
            @Html.DisplayFor(modelItem => item.DittaFornitrice.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsoProdotti.Uso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeProdotto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idProdotto }) |
       
            @Html.ActionLink("Delete", "Delete", new { id=item.idProdotto })
        </td>
    </tr>
}

</table>
